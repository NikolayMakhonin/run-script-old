{
	"name": "@flemist/run-script",
	"version": "0.2.5",
	"description": "Use JavaScript instead npm scripts",
	"author": {
		"name": "Nikolay Makhonin",
		"url": "https://github.com/NikolayMakhonin/"
	},
	"homepage": "https://github.com/NikolayMakhonin/run-script/",
	"bugs": {
		"url": "https://github.com/NikolayMakhonin/run-script/issues"
	},
	"repository": {
		"type": "git",
		"url": "git+ssh://git@github.com:NikolayMakhonin/run-script.git"
	},
	"license": "CC0-1.0",
	"keywords": [
		"npm",
		"run",
		"script",
		"scripts",
		"cli",
		"execute",
		"run-script",
		"javascript",
		"js",
		"parallel",
		"sequential",
		"helpers",
		"utils",
		"tools",
		"shell",
		"ci"
	],
	"files": [
		"dist/**"
	],
	"types": "dist/types/node/index.d.ts",
	"main": "dist/dev/js/main/node/index.js",
	"jsnext:main": "dist/dev/mjs/main/node/index.js",
	"module": "dist/dev/mjs/main/node/index.js",
	"bin": {
		"run-script": "./dist/dev/js/main/node/run.js"
	},
	"engines": {
		"node": ">= 8.6.0"
	},
	"scripts": {
		"precommit": "npm run build-test:ci",
		"prepublishOnly": "npm run build-test:dev",
		"ci": "npm run build-test:ci && ncat \"tmp/*/coverage/all/lcov/**/lcov.info\" --banner \"\" | coveralls",
		"lint": "node ./env/scripts/run.js \"require('./env/scripts/common').lint().stopOnError()\"",
		"lint:fix": "node ./env/scripts/run.js \"require('./env/scripts/common').lint({fix: true}).stopOnError()\"",
		"-------- build --------": "",
		"build:dev": "node ./env/scripts/run.js \"require('./env/scripts').buildAll(['dev']).stopOnError()\"",
		"-------- test --------": "",
		"test:dev": "node ./env/scripts/run.js \"require('./env/scripts').testAll(['dev'], { build: false }).stopOnError()\"",
		"test:all": "node ./env/scripts/run.js \"require('./env/scripts').testAll(['demo', 'dev', 'stage', 'prod'], { build: false }).stopOnError()\"",
		"test:ci": "node ./env/scripts/run.js \"require('./env/scripts').testCiAll(['dev'], { build: false }).stopOnError()\"",
		"-------- build-test --------": "",
		"build-test:dev": "node ./env/scripts/run.js \"require('./env/scripts').testAll(['dev']).stopOnError()\"",
		"build-test:all": "node ./env/scripts/run.js \"require('./env/scripts').testAll(['demo', 'dev', 'stage', 'prod']).stopOnError()\"",
		"build-test:ci": "node ./env/scripts/run.js \"require('./env/scripts').testCiAll(['dev']).stopOnError()\"",
		"-------- coverage --------": "",
		"coverage:check:dev": "node ./env/scripts/run.js \"require('./env/scripts/specific/tests').coverageCheck('dev').stopOnError()\"",
		"coverage:mocha:dev": "node ./env/scripts/run.js \"require('./env/scripts/specific/tests').coverageMocha('dev').stopOnError()\"",
		"coverage:merge:dev": "node ./env/scripts/run.js \"require('./env/scripts/specific/tests').coverageMerge('dev').stopOnError()\""
	},
	"devDependencies": {
		"@babel/cli": "^7.11.6",
		"@babel/core": "^7.11.6",
		"@babel/plugin-proposal-class-properties": "^7.10.4",
		"@babel/plugin-proposal-export-default-from": "^7.10.4",
		"@babel/plugin-proposal-export-namespace-from": "^7.10.4",
		"@babel/plugin-proposal-optional-chaining": "^7.11.0",
		"@babel/plugin-proposal-throw-expressions": "^7.10.4",
		"@babel/plugin-syntax-dynamic-import": "^7.2.0",
		"@babel/plugin-transform-async-to-generator": "^7.10.4",
		"@babel/plugin-transform-parameters": "^7.10.5",
		"@babel/plugin-transform-runtime": "^7.11.5",
		"@babel/plugin-transform-typescript": "^7.11.0",
		"@babel/preset-env": "^7.11.5",
		"@babel/register": "^7.11.5",
		"@rollup/plugin-alias": "^3.1.1",
		"@rollup/plugin-babel": "^5.2.1",
		"@rollup/plugin-commonjs": "^15.0.0",
		"@rollup/plugin-json": "^4.0.3",
		"@rollup/plugin-node-resolve": "^9.0.0",
		"@types/chai": "^4.2.14",
		"@types/mocha": "^8.0.4",
		"@types/node": "^14.14.3",
		"@types/yargs": "^15.0.9",
		"@typescript-eslint/eslint-plugin": "^4.3.0",
		"@typescript-eslint/parser": "^4.3.0",
		"babel-plugin-root-import": "^6.5.0",
		"browserslist": "^4.14.1",
		"caller": "^1.0.1",
		"caniuse-lite": "^1.0.30001124",
		"chai": "^4.2.0",
		"chai-as-promised": "^7.1.1",
		"chokidar": "^3.4.2",
		"color": "^3.1.2",
		"core-js": "^3.2.1",
		"core-js-pure": "^3.2.1",
		"cpy-cli": "^3.1.1",
		"depcheck": "^1.2.0",
		"eslint": "^7.10.0",
		"eslint-config-pro": "^2.0.5",
		"eslint-plugin-html": "^6.1.0",
		"eslint-plugin-markdown": "^1.0.2",
		"eslint-plugin-node": "^11.1.0",
		"eslint-plugin-sonarjs": "^0.5.0",
		"eslint-plugin-svelte3": "^2.7.3",
		"fs-extra": "^9.0.1",
		"globby": "^11.0.1",
		"json5": "^2.1.0",
		"mocha": "^8.1.3",
		"ncat": "^4.0.2",
		"nyc": "^15.1.0",
		"regenerator-runtime": "^0.13.7",
		"require-from-memory": "^1.0.2",
		"rollup": "^2.27.1",
		"rollup-plugin-istanbul": "^2.0.1",
		"rollup-plugin-node-builtins": "^2.1.2",
		"rollup-plugin-node-globals": "^1.4.0",
		"rollup-plugin-node-polyfills": "^0.2.1",
		"rollup-plugin-replace": "^2.2.0",
		"rollup-plugin-terser": "^7.0.2",
		"rollup-pluginutils": "^2.8.2",
		"shx": "^0.3.2",
		"typescript": "^4.0.3",
		"webrain": "^1.4.6-a"
	},
	"dependencies": {
		"yargs": "^16.1.0",
		"@babel/runtime-corejs3": "^7.11.2",
		"ps-tree": "^1.2.0",
		"kleur": "^4.1.1"
	}
}
